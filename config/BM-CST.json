{
  "method": "PT",
  "multicore": true,
  "num_processors": 0,
  "golden_rows": true,
  "add_base_path": true,
  "dataset": "BM",
  "dataset_path": "/data/autojoin-Benchmark/",
  "output_path": "/output/BM/CST/",
  "override": true,
  "do_remaining": true,
  "aj_subset_size": 2,
  "aj_num_subset": 6,

  "pt_params": {
    "max_tokens": 3,
    "max_blocks": 3,
    "generalize": false,
    "token_splitters": [" "],
    "remove_duplicate_patterns": true,
    "switch_literals_placeholders": true,
    "only_first_match": false,
    "units_to_extract": ["LiteralPatternBlock", "PositionPatternBlock", "TokenPatternBlock", "SplitSubstrPatternBlock"],
    "prioritize_equal_coverage_patterns": true,
    "extended_transformations": false,
    "sample_size": ["100%"],
    "extension_mode": "None",
    "extension_index": 2
  }
}
